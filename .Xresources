!URxvt.font:		xft:文泉驿等宽正黑:medium:pixelsize=12:antialias=True
!URxvt.font:		xft:Bitstream Vera Sans Mono:medium:pixelsize=12:antialias=True
URxvt.font:		xft:inconsolata:medium:pixelsize=14:antialias=True
URxvt.color0:		grey20	
URxvt.perl-ext-common:	default,tabbed
Xft.antialias:1
Xft.hinting:1
Xft.hintstyle:hintfull
Xft.rgba:vrgb
XTerm*faceName:monospace
!Emacs.Font:monospace 11
!Xcursor.theme:Vanilla-DMZ
Xcursor.theme:Neutral
URxvt*perl-ext: default,matcher,tabbedex,url-select
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.underlineURLs: true
URxvt*urlLauncher:opera-devel
