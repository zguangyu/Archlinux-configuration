emacs.title:Emacs only for Guangyu
emacs.BorderWidth:0

Xft.dpi:96
!Xcursor.theme:	Obsidian
!! RXVT-unicode setting
URxvt.depth:		32
URxvt.buffered:         true
!URxvt.depth:            32
!URxvt.background:       rgba:0Af0/0Af0/0Af0/dddd
!URxvt.foreground:       white
URxvt.geometry:		100x50
!URxvt.background:	grey20
!URxvt.background:	rgba:ffff/ffff/ffff/dddd
!urxvt*depth:32
urxvt*background:rgba:2000/2000/2000/cccc
URxvt.foreground:	white	
URxvt.scrollBar:	false	
!URxvt.scrollstyle:	plain
URxvt.loginSheel:	false
! Use shift+pageup/down to scroll in screen
URxvt.secondaryScroll:	True
URxvt.transparent:	true	
URxvt.tintColor:	white	
URxvt.shading:		50
URxvt.saveLines:	5000
URxvt.preeditType:	Root
URxvt.borderLess:	false	
URxvt.title:		Rxvt-Unicode
!URxvt.boldFont:		xft:文泉驿等宽正黑:bold:pixelsize=12:antialias=True
!URxvt.boldFont:		xft:Bitstream Vera Sans Mono:bold:pixelsize=12:antialias=True
